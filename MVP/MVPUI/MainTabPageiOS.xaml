<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Microsoft.Mvpui.MainTabPageiOS"
            xmlns:local="clr-namespace:Microsoft.Mvpui;assembly=Mvpui">
  <!--Pages can be added as references or inline-->
    <NavigationPage Icon="icon_profile.png"
      Title="My Profile">
        <x:Arguments>
            <local:MyProfile/>
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Icon="icon_contribution.png"
      Title="Contributions">
        <x:Arguments>
            <local:ContributionsPage/>
        </x:Arguments>
    </NavigationPage>
</TabbedPage>